<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"

        tools:context=".Result">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="339dp"
            android:layout_height="682dp" android:id="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:textColor="#000000"
            android:textSize="22dp"

            android:layout_marginTop="24dp" app:layout_constraintTop_toTopOf="parent">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="62dp">
            <TextView
                    android:text="ผลการประเมิน"
                    android:layout_width="234dp"
                    android:layout_height="match_parent" android:id="@+id/textView16" android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:textSize="30dp"
                    android:fontFamily="@font/fontstyle"
                    android:backgroundTint="#ED030303"
                    android:paddingLeft="20dp"
            />
            <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" app:srcCompat="@drawable/info4" android:id="@+id/imageButton2"
                    android:backgroundTint="#02039BE5"
                    android:layout_gravity="right"
            />
        </LinearLayout>
        <Space
                android:layout_width="match_parent"
                android:layout_height="17dp"/>
        <ListView
                android:layout_width="match_parent"
                android:layout_height="580dp" android:id="@+id/results"
                android:textColor="#000000"
                android:textSize="22dp"
        >
        </ListView>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>