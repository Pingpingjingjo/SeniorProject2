<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Result">
    <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" app:srcCompat="@drawable/result1" android:id="@+id/imageView5"
            tools:layout_editor_absoluteX="41dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="324dp"
            android:layout_height="521dp" android:id="@+id/linearLayout"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="110dp">
            <ImageView
                    android:layout_width="204dp"
                    android:layout_height="106dp" app:srcCompat="@drawable/depress" android:id="@+id/imageView6"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:scaleType="fitEnd"/>
            <TextView
                    android:text="หกดหก"
                    android:layout_width="146dp"
                    android:layout_height="106dp" android:id="@+id/resultD"
                    android:gravity="center_vertical"
            />
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="102dp">
            <ImageView
                    android:layout_width="204dp"
                    android:layout_height="102dp" app:srcCompat="@drawable/anxie" android:id="@+id/imageView8"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="157dp"
                    android:layout_height="match_parent" android:id="@+id/resultA"
                    android:gravity="center_vertical"
            />
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="105dp">
            <ImageView
                    android:layout_width="207dp"
                    android:layout_height="105dp" app:srcCompat="@drawable/social" android:id="@+id/imageView9"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="152dp"
                    android:layout_height="match_parent" android:id="@+id/resultS"
                    android:gravity="center_vertical"
            />
        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>